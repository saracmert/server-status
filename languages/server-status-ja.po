#. Plugin Name of the plugin/theme
#: server-status.php:44
msgid "Server Status"
msgstr "Server Status"

#: server-status.php:54
msgid "This plugin is not compatible with this OS!!(ID: %s)"
msgstr "このプラグインと互換性のないOSを使用しています!!(識別子: %s)"

#: server-status.php:81
msgid "If you have any comment, add it above this line"
msgstr "コメントがあればこの線より上に追加してください。"

#: server-status.php:120
msgid "Cache Expiration was set for %s sec."
msgstr "キャッシュの有効期限が %s 秒に設定されました。"

#: server-status.php:122
msgid "Using Cached Data"
msgstr "キャッシュデータを使用中です。"

#: server-status.php:149
msgid "Enable Colorize"
msgstr "色付けを有効化"

#: server-status.php:153
msgid "Cached Data Expiration: %s sec."
msgstr "キャッシュデータの有効期限: %s 秒"

#: server-status.php:155
msgid "Min: 2 sec, Step: 1 sec"
msgstr "最小: 2秒・単位: 1秒"

#: server-status.php:197
msgid ""
"You are using unsupported PHP branch(%s). We recommend upgrading immediately."
msgstr ""
"サポートされていないPHPのブランチ(%s)を使用しています。ただちにアップグレード"
"されることをお勧めします。"

#: server-status.php:306
msgid "Uptime data is unavailable!"
msgstr "稼働時間情報は利用できません"

#: server-status.php:326 server-status.php:362
msgid "%d user"
msgid_plural "%d users"
msgstr[0] "%d ユーザ"

